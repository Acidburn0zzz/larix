{
  "name": "@larix/generator",
  "version": "0.1.4",
  "description": "Larix apps generator",
  "author": "Victor Vlasenko <victor.vlasenko@sysgears.com>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sysgears/larix.git"
  },
  "bugs": {
    "url": "https://github.com/sysgears/larix/issues"
  },
  "homepage": "https://github.com/sysgears/larix/tree/master/packages/generator#readme",
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "pnpify tsc",
    "watch": "pnpify tsc -w",
    "lint": "tslint -p tsconfig.json --fix -c ../../tslint.json",
    "tests": "jest",
    "test": "yarn tests && yarn lint",
    "precommit": "lint-staged",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts",
    "lib/*.map"
  ],
  "dependencies": {
    "chalk": "^2.4.1",
    "fuzzy": "^0.1.3",
    "humps": "^2.0.1",
    "inquirer": "^5.2.0",
    "inquirer-autocomplete-prompt": "^0.12.2",
    "minimist": "^1.2.0",
    "mustache": "2.3.0",
    "source-map-support": "^0.5.6",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@types/humps": "^1.1.2",
    "@types/mustache": "^0.8.30",
    "@yarnpkg/pnpify": "^2.0.0-rc.3",
    "rimraf": "^2.6.2",
    "typescript": "^3.3.3"
  }
}
